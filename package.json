    "jsonlint-mod": "^1.7.6",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.0.0",
    "umi": "^4.2.5"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.41",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "monaco-editor": "^0.48.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
